AD250000  // sw $5, 0($9)         # Mem[100] = $5 = 15
AD260004  // sw $6, 4($9)         # Mem[104] = $6 = 10
8D2A0000  // lw $10, 0($9)        # $10 = Mem[100] = 15
8D2B0004  // lw $11, 4($9)        # $11 = Mem[104] = 10
